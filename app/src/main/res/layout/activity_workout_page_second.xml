<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"

    android:id="@+id/textViewtest"

    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.WorkoutPageSecond"
    tools:layout_editor_absoluteX="16dp"
    tools:layout_editor_absoluteY="-58dp">


    <!--NAVBAR STUFF-->


    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottomNavigationView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:menu="@menu/bottom_nav_menu" />


    <!--NAVBAR STUFF-->


    <TextView
        android:id="@+id/textViewTest"
        android:layout_width="224dp"
        android:layout_height="43dp"
        android:layout_marginStart="108dp"
        android:layout_marginTop="40dp"
        android:text="@string/Title"
        android:textAlignment="center"
        android:textSize="34sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/workoutTitle1"
        android:layout_width="88dp"
        android:layout_height="20dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="124dp"
        android:text="workoutTitle1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/description2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="description2"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="343dp" />

    <TextView
        android:id="@+id/workoutTitle2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="workoutTitle2"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="300dp" />

    <TextView
        android:id="@+id/routine1"
        android:layout_width="88dp"
        android:layout_height="20dp"
        android:layout_marginTop="24dp"
        android:text="routine1"
        app:layout_constraintStart_toStartOf="@+id/description1"
        app:layout_constraintTop_toBottomOf="@+id/description1" />

    <TextView
        android:id="@+id/description1"
        android:layout_width="88dp"
        android:layout_height="20dp"
        android:layout_marginTop="24dp"
        android:text="description1"
        app:layout_constraintStart_toStartOf="@+id/workoutTitle1"
        app:layout_constraintTop_toBottomOf="@+id/workoutTitle1" />

    <TextView
        android:id="@+id/routine2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="routine2"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="394dp" />

    <TextView
        android:id="@+id/workoutTitle3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="workoutTitle3"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="461dp" />

    <TextView
        android:id="@+id/description3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="description3"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="507dp" />

    <TextView
        android:id="@+id/routine3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="routine3"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="554dp" />

    <pl.droidsonroids.gif.GifImageView
        android:id="@+id/workoutGif"
        android:layout_width="179dp"
        android:layout_height="121dp"
        android:layout_marginStart="136dp"
        android:layout_marginTop="112dp"
        android:src="@drawable/chest1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/chest1" />

    <pl.droidsonroids.gif.GifImageView
        android:id="@+id/workoutGif2"
        android:layout_width="179dp"
        android:layout_height="121dp"
        android:layout_marginTop="48dp"
        android:src="@drawable/chest1"
        app:layout_constraintStart_toStartOf="@+id/workoutGif"
        app:layout_constraintTop_toBottomOf="@+id/workoutGif"
        app:srcCompat="@drawable/chest1" />

    <pl.droidsonroids.gif.GifImageView
        android:id="@+id/workoutGif3"
        android:layout_width="179dp"
        android:layout_height="121dp"
        android:layout_marginTop="48dp"
        android:src="@drawable/chest1"
        app:layout_constraintStart_toStartOf="@+id/workoutGif2"
        app:layout_constraintTop_toBottomOf="@+id/workoutGif2"
        app:srcCompat="@drawable/chest1" />


</androidx.constraintlayout.widget.ConstraintLayout>